#include <string>
#include <cstddef>

struct dllCell {
  std::string val;
  dllCell *next2;
  dllCell *prev2;
};

struct cell{
  int priority;
  dllCell* queue;
  cell *next;
};

typedef dllCell *dll;
typedef cell *list;

const list emptyList=nullptr;

/*************************************************/
/* Funzione da implementare                      */
